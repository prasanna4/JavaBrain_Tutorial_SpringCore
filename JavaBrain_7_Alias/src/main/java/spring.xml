<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd" >
 
    <bean id="triangle" class="org.javabrains.springcore.JavaBrain_7_Alias.Triangle">
      	<property name="pointA" ref="point0"> 
      	</property>	 
      	
      	<property name="pointB" >
      		<bean class="org.javabrains.springcore.JavaBrain_7_Alias.Point">
		      <property name="x" value="-20"></property>
		      <property name="y" value="0"></property>     		 
		    </bean>
      	</property>	 
      	<property name="pointC" >
	      	<bean class="org.javabrains.springcore.JavaBrain_7_Alias.Point">
		      <property name="x" value="0"></property>
		      <property name="y" value="20"></property>     		 
		    </bean>
      	</property>     	
    </bean>
    
    <bean id="point0" class="org.javabrains.springcore.JavaBrain_7_Alias.Point">
      <property name="x" value="0"></property>
      <property name="y" value="0"></property>     		 
    </bean>
    
    <alias name="triangle" alias="triangle-alias"/>
     
    
    
</beans>
