<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd" >
    
    <bean class="org.springframework.beans.factory.annotation.AutowiredAnnotationBeanPostProcessor">
    </bean>
      
    <bean id="circle" class="org.javabrains.springcore.JavaBrain_20_Autowired_Annotation_Part2.Circle">

    </bean>
    
    <bean id="center" class="org.javabrains.springcore.JavaBrain_20_Autowired_Annotation_Part2.Point">
      <property name="x" value="0"></property>
      <property name="y" value="0"></property>     		 
    </bean>
    
    <bean id="pointB" class="org.javabrains.springcore.JavaBrain_20_Autowired_Annotation_Part2.Point">
      <property name="x" value="-20"></property>
      <property name="y" value="0"></property>     		 
    </bean>
    <bean id="pointC" class="org.javabrains.springcore.JavaBrain_20_Autowired_Annotation_Part2.Point">
      <property name="x" value="0"></property>
      <property name="y" value="-20"></property>     		 
    </bean>     


      
    
</beans>
