<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd" >
 
    <bean id="triangle" class="org.javabrains.springcore.JavaBrain_10_Using_Collection_List.Triangle">
      	<property name="points" >
      	  <list>
      	        <ref bean="point0"></ref>
      	        <ref bean="point1"></ref>
      	        <ref bean="point2"></ref>
      	  </list>
      	</property>	    	
    </bean>
    
    <bean id="point0" class="org.javabrains.springcore.JavaBrain_10_Using_Collection_List.Point">
      <property name="x" value="0"></property>
      <property name="y" value="0"></property>     		 
    </bean>
    
    <bean id="point1" class="org.javabrains.springcore.JavaBrain_10_Using_Collection_List.Point">
      <property name="x" value="-20"></property>
      <property name="y" value="0"></property>     		 
    </bean>
     
    <bean id="point2" class="org.javabrains.springcore.JavaBrain_10_Using_Collection_List.Point">
      <property name="x" value="0"></property>
      <property name="y" value="20"></property>     		 
    </bean>
    
     
    
    
</beans>
